SELECT 
    location,
    current_speed_kmh,
    free_flow_speed_kmh,
    ROUND(speed_ratio, 2) AS speed_ratio,
    confidence
FROM traffic_data
WHERE confidence &gt; 0.95
  AND speed_ratio &lt; 0.85
group by location
order by speed_ratio desc ;
 
 
 
SELECT 
    id,
    timestamp,
    location,
    current_speed_kmh,
    free_flow_speed_kmh,
    speed_ratio,
    traffic_level
FROM traffic_data
WHERE traffic_level IN ('DONG', 'KET_XE')
ORDER BY speed_ratio ASC;




SELECT 
    location,
    COUNT(*) AS total_records,
    ROUND(AVG(speed_ratio), 2) AS avg_speed_ratio,
    CASE
        WHEN AVG(speed_ratio) &gt;= 0.8 THEN 'THOANG'
        WHEN AVG(speed_ratio) &gt;= 0.5 THEN 'DONG'
        ELSE 'KET_XE'
    END AS avg_traffic_level
FROM traffic_data
GROUP BY location;


SELECT 
    location,
    COUNT(*) AS total_records,
    ROUND(AVG(speed_ratio), 2) AS avg_speed_ratio,
	traffic_level
FROM traffic_data
GROUP BY traffic_level,location;

SELECT 
    location,
    hour || ':00 - ' || hour || ':59' AS time_slot,
    congestion_count
FROM (
    SELECT
        location,
        strftime('%H', timestamp) AS hour,
        COUNT(*) AS congestion_count,
        RANK() OVER (
            PARTITION BY location
            ORDER BY COUNT(*) DESC
        ) AS rnk
    FROM traffic_data
    WHERE
        (location LIKE '%BỆNH VIỆN%' OR location LIKE '%BV%')
        AND speed_ratio &lt;= 0.8
    GROUP BY location, hour
)
WHERE rnk = 1;</sql><current_tab id="0"/></tab_sql></sqlb_project>
